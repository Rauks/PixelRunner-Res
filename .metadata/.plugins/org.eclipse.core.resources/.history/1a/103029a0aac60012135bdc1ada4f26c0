package xmlLevelEncoder;

import java.util.HashMap;

public class ObjectL {

	ArrayList<String, String> hm;
	
	public ObjectL(){
		this.hm = new HashMap<>();
	}
	
	public void add(String object, String layer){
		this.hm.put(object, layer);
	}
	
	public HashMap<String, String> get(){
		return this.hm;
	}
	
	public int size(){
		return this.hm.size();
	}
}
